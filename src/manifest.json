{
  "manifest_version": 2,

  "name": "Noodes",
  "description": "Notes extension",
  "version": "1.0",
  "background": {
   "scripts": ["popup.js"],
   "persistent": false
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
   
  },
  "permissions": [
   "activeTab",
   "storage"
   ]

}